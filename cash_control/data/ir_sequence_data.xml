<odoo>
    <!-- Secuencia para Movimientos de Caja -->
    <record id="seq_cash_control_movement" model="ir.sequence">
        <field name="name">Cash Movement</field>
        <field name="code">cash.control.movement</field>
        <field name="prefix">MOV</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Secuencia para Movimientos Bancarios -->
    <record id="seq_cash_control_bank_movement" model="ir.sequence">
        <field name="name">Bank Movement</field>
        <field name="code">cash.control.bank.movement</field>
        <field name="prefix">BKMOV</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
